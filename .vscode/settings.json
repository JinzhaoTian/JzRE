    "C_Cpp.intelliSenseEngine": "disabled",
    "C_Cpp.default.cStandard": "c17",
    "C_Cpp.default.cppStandard": "c++20",
    "files.associations": {
        "*.css": "tailwindcss",
        "*.mm": "objective-cpp",
        "*.h": "c",
        "*.hpp": "cpp",
        "*.cpp": "cpp",
